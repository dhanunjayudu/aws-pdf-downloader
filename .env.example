# NCDHHS Python Service Environment Configuration
# Copy this file to .env and update with your values

# AWS Configuration
AWS_REGION=us-east-1
AWS_PROFILE=default

# S3 Configuration
S3_BUCKET_NAME=ncdhhs-cwis-policy-manuals-python

# DynamoDB Configuration
EMBEDDINGS_TABLE=ncdhhs-embeddings
INTERACTIONS_TABLE=ncdhhs-interactions

# Flask Configuration
FLASK_ENV=development
FLASK_DEBUG=True
PORT=5000

# Bedrock Configuration (when enabled)
BEDROCK_REGION=us-east-1
BEDROCK_MODEL_ID=anthropic.claude-3-haiku-20240307-v1:0
EMBEDDING_MODEL_ID=amazon.titan-embed-text-v1

# Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s
